            <section id="main-content" class=" ">
            <section class="wrapper main-wrapper row" >
                <div class='col-xs-12'>
                    <div class="page-title">
                        <div class="pull-left">
                            <h1 class="title">Sửa Thành Viên</h1>
                        </div>
                        <div class="pull-right hidden-xs">
                            <% if (message.length > 0) { %>
                                <div class="alert alert-danger"><%= message
                                   %></div>
                            <% } %>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="col-xs-12">
                    <section class="box ">
                        <header class="panel_header">
                            <h2 class="title pull-left">Thông Tin</h2>
                            <div class="actions panel_actions pull-right">
                                <a class="box_toggle fa fa-chevron-down"></a>
                                <a class="box_setting fa fa-cog" data-toggle="modal" href="#section-settings"></a>
                                <a class="box_close fa fa-times"></a>
                            </div>
                        </header>
                        <div class="content-body">
                            <div class="row">
                                <form action="" role="form" enctype="multipart/form-data" method="POST">
                                    <% if (message.length > 0) { %>
                                        <div class="alert alert-danger"><%= message %></div>
                                    <% } %>
                                        <div class="col-xs-12 col-sm-9 col-md-8 padding-bottom-30">
                                            <div class="form-group">
                                                <label class="form-label" for="field-120334">Name</label>
                                                <span class="desc"></span>
                                                <div class="controls">
                                                    <input type="text" id="username" value="<%= users.username %>" class="form-control" name="username">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-9 col-md-8 padding-bottom-30">
                                            <div class="form-group">
                                                <label class="form-label" for="field-120334">fullname</label>
                                                <span class="desc"></span>
                                                <div class="controls">
                                                    <input type="text" id="fullname" value="<%= users.fullname %>" class="form-control" name="fullname">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-9 col-md-8 padding-bottom-30">
                                            <div class="form-group">
                                                <label class="form-label" for="field-120334">Hình</label>
                                                <span class="desc"></span><br/>
                                                <img src="/../uploads/user/<%= users.images%>" width="200px">
                                                <div class="controls">
                                                    <input type="file" id="images" value="" required="required" class="form-control" name="images">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-9 col-md-8 padding-bottom-30">
                                            <div class="form-group">
                                                <label class="form-label" for="field-120334">email</label>
                                                <span class="desc"></span>
                                                <div class="controls">
                                                    <input type="text" id="email" value="<%= users.email %>" class="form-control" name="email">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-9 col-md-8 padding-bottom-30">
                                            <div class="form-group">
                                                <label class="form-label" for="field-120334">Phone</label>
                                                <span class="desc"></span>
                                                <div class="controls">
                                                    <input type="text" id="phoneNumber" value="<%= users.phoneNumber %>" class="form-control" name="phoneNumber">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-9 col-md-8 padding-bottom-30">
                                            <div class="form-group">
                                                <label class="form-label" for="field-120334">address</label>
                                                <span class="desc"></span>
                                                <div class="controls">
                                                    <input type="text" id="address" value="<%= users.address %>" class="form-control" name="address">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-9 col-md-8 padding-bottom-30">
                                            <div class="form-group">
                                                <label class="form-label" for="field-120334">password</label>
                                                <span class="desc"></span>
                                                <div class="controls">
                                                    <input type="password" id="password" value="" class="form-control" name="password">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-9 col-md-8 padding-bottom-30">
                                            <div class="form-group">
                                                <label class="form-label" for="field-120334">password2</label>
                                                <span class="desc"></span>
                                                <div class="controls">
                                                    <input type="password" id="password2" value="" class="form-control" name="password2">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-9 col-md-8 padding-bottom-30">
                                            <div class="form-group">
                                                <label class="form-label" for="field-120334">Loại Sản Phẩm</label>
                                                <span class="desc"></span>
                                                <div class="controls">         
                                                    <select class="form-control" id="cate" name="PhanQuyen">
                                                        <% phq.forEach(function(phq){ %> 
                                                            <option value="<%= phq.id%>"<%= (phq.id == users.PhanQuyen) ? 'selected=\"selected\"' : ''%></option><%= phq.TenQuyen%></option>
                                                        <%})%>
                                                    </select>
            
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-12 col-sm-9 col-md-8 padding-bottom-30">
                                            <div class="text-left">
                                                <button type="submit" class="btn btn-primary">Save</button>
                                                <button type="button" class="btn">Cancel</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                    </section>
                    </div>
            </section>
        </section>
