<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../playout/head %>
</head>
<body>
        <div class="page-container row-fluid container-fluid"></div>
            <% include ./../playout/menu %>

            <section id="main-content" class=" ">
                <section class="wrapper main-wrapper row">
                    <div class='col-xs-12'>
                        <div class="page-title">
                            <div class="pull-left">
                                <h1 class="title">Dashboard</h1>
                          
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                   
                    <div class="clearfix"></div>
                    <div class="col-lg-12">
                        <section class="box nobox marginBottom0">
                            <div class="content-body">
                                <div class="row">
                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                        <div class="r4_counter db_box">
                                            <i class='pull-left fa fa-thumbs-up icon-md icon-rounded icon-primary'></i>
                                            <div class="stats">
                                                <h4><strong><%=totaloder%></strong></h4>
                                                <span>Orders</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                        <div class="r4_counter db_box">
                                            <i class='pull-left fa fa-shopping-cart icon-md icon-rounded icon-accent'></i>
                                            <div class="stats">
                                                <h4><strong><%= total%></strong></h4>
                                                <span>Hàng Tồn Kho</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                        <div class="r4_counter db_box">
                                            <i class='pull-left fa fa-dollar icon-md icon-rounded icon-purple'></i>
                                            <div class="stats">
                                                <h4><strong><%= totalcart%></strong></h4>
                                                <span>Tiền Hàng VNĐ</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 col-sm-6 col-xs-12">
                                        <div class="r4_counter db_box">
                                            <i class='pull-left fa fa-users icon-md icon-rounded icon-warning'></i>
                                            <div class="stats">
                                                <h4><strong><%= totaluser%></strong></h4>
                                                <span>Thành Viên</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- End .row -->    
                            </div>
                        </section>
                    </div>
                    <div class="col-xs-12">
                        <section class="box ">
                            <header class="panel_header">
                                <h2 class="title pull-left">Thống Kê</h2>
                                <div class="actions panel_actions pull-right">
                                    <a class="box_toggle fa fa-chevron-down"></a>
                                    <a class="box_setting fa fa-cog" data-toggle="modal" href="#section-settings"></a>
                                    <a class="box_close fa fa-times"></a>
                                </div>
                            </header>
                            <div class="content-body">
                                <div class="row">
                                    <div class="col-xs-12">
                                        <div class="wid-vectormap">
                                            <div class="row">
                                                <div class="col-xs-12 col-sm-12">
                                                    <figure>
                                                        <div id="db-world-map-markers" style="width: 100%; height: 300px">
                                                            <table id="example" class="display table table-hover table-condensed">
                                                                <thead>
                                                                    <tr>
                                                                        <th>ID</th>
                                                                        <th>Tiêu Đề</th>
                                                                        <th>Bình Luận</th>
                                                                        <th>email</th>
                                                                       
                                                                        <th>Họ Tên</th>
                                                                        
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <% var id = 0 %>
                                                                    <% lienher.forEach(function(lienher){ id ++%>
                                                                    <tr>
                                                                        <td><%= id%></td>
                                                                        <td><%= lienher.TieuDe%></td>
                                                                        <td><%= lienher.BinhLuan%></td>
                                                                        <td><%= lienher.email%></td>
                                                                        <td><%= lienher.HoTen%></td>
                                                                     
                                                                       
                                                                      
                                                                    </tr>
                                                                    <%})%>
                                                                </tbody>
                                                            </table></div>
                                                    </figure>
                                                </div>
                                          
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- End .row -->
                            </div>
                        </section>
                    </div>
                </section>
            </section>
            <!-- END CONTENT -->
            <div class="chatapi-windows ">
            </div>
            <% include ../playout/footer %>
</body>
</html>