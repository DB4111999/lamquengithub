<% include ../playout/head %>
        <!-- END TOPBAR -->
        <!-- START CONTAINER -->
        <div class="page-container row-fluid container-fluid">
            <!-- SIDEBAR - START -->
           
                 <% include ../playout/menu.ejs %>


            <!--  SIDEBAR - END -->
            <!-- START CONTENT -->
            <section id="main-content" class=" ">
                <section class="wrapper main-wrapper row">
                    <div class='col-xs-12'>
                        <div class="page-title">
                            <div class="pull-left">
                                <h1 class="title">Danh Sách Nhà Cung Cấp</h1>
                            </div>
                            <div class="pull-right hidden-xs">
                               
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="col-lg-12">
                        <section class="box ">
                            <header class="panel_header">
                                <h2 class="title pull-left">All Cate</h2>
                                <div class="actions panel_actions pull-right">
                                    <a class="box_toggle fa fa-chevron-down"></a>
                                    <a class="box_setting fa fa-cog" data-toggle="modal" href="#section-settings"></a>
                                    <a class="box_close fa fa-times"></a>
                                </div>
                            </header>
                            <div class="content-body">
                                <div class="row">
                                    <div class="col-xs-12">
                                        <% if (success_msg.length > 0) { %>
                                            <div class="alert alert-success"><%= success_msg %></div>
                                          <% } %>
                                        <table id="example" class="display table table-hover table-condensed">
                                            <thead>
                                                <tr>
                                                    <th>ID</th>
                                                    <th>Công Ty</th>
                                                    <th>Địa Chỉ</th>
                                                   
                                                    <th>website</th>
                                                    <th>PhanPhoi</th>

                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% var id = 0 %>
                                                <% ncc.forEach(function(ncc){ id ++%>
                                                <tr>
                                                    <td><%= id%></td>
                                                    <td><%= ncc.CongTy%></td>
                                                    <td><%= ncc.DiaChi%></td>
                                                    <td><%= ncc.Phone%></td>
                                                    <td><%= ncc.website%></td>
                                                    <td><%= ncc.PhanPhoi%></td>
                                                    <td>
                                                        <a href="./deletencc/<%= ncc.id %>">Xoá</a> | 
                                                        <a href="./getupdatencc/<%= ncc._id %>">Sửa</a>
                                                    </td>
                                                </tr>
                                                <%})%>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                </section>
            </section>
            <!-- END CONTENT -->
            <div class="chatapi-windows ">

            </div>

<% include ../playout/footer.ejs %>
